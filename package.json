{
  "name": "react-boilerplate",
  "version": "3.1.0",
  "description": "A minimal boilerplate with hot module replacement",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config=webpack.dev.js",
    "build": "yarn run clean-dist && webpack -p --config=webpack.prod.js",
    "clean-dist": "rm -f -r -d dist",
    "start-prod": "yarn run build && node express.js"
  },
  "author": "bernardo de stefano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:bernardodestefano/react-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "boilerplate",
    "react",
    "babel",
    "sass",
    "hmr"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "source-map-loader": "^0.2.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-merge": "^4.1.4"
  },
  "bugs": {
    "url": "https://github.com/bernardodestefano/react-boilerplate/issues"
  },
  "homepage": "https://github.com/bernardodestefano/react-boilerplate#readme"
}
